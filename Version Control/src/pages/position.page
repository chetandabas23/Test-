<apex:page controller="position" tabStyle="Position__c" >
    <apex:form >
    <apex:pageBlock title="Positions">
        <apex:pageBlockSection >
        <apex:inputField value="{!pstn.name}"/>
                <apex:inputField value="{!pstn.Status__c}"/>
                <apex:inputField value="{!pstn.Start_Date__c}"/>
                <apex:inputField value="{!pstn.End_Date__c}"/>
        </apex:pageBlockSection>
        <apex:commandButton action="{!save}" value="save"/>
        </apex:pageBlock>
    </apex:form>
</apex:page>